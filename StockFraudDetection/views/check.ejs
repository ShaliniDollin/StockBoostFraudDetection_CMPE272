<body>

    
    <!-- START PRELOADER -->
    <div class="ole">
    
        <section id="jSplash">
        
            <div id="circle"></div>
            
        </section>
        
    </div>
    <!-- END PRELOADER -->
   
   <!-- START HOME -->
   <div class="page nopad" id="home">
    
        <header>   
           
             <div class="container">
               
                   <div class="row">
                                                  
                       <div class="span12">
                           
                           <div class="logo">  
                            <a class="brand" href="#">STOCK FRAUD ANALYZER</a><br />
                            <span class="slogan">Your Advisor :)</span>
                         </div>
                     
                        </div> <!-- end span12 -->
            
                  </div> <!-- end row -->
              
             </div> <!-- end container -->
       
       </header>  <!-- end header -->
     
       <div class="top-nav"> 
       
         <div class="container">
         
            <div class="row">
         
               <div class="span12">
                  
                  <nav id="nav-button">
                    <div id="menu-button">
                       <div id="menu-button-inner"><i class=" icon-align-justify"></i>Menu</div>
                    </div>
                   </nav> 
                   
                   <div class="menu-wrap">
                    
                     <nav id="menu">
                                                  
                          <ul id="navigation">
                          
                            
                           <li>
                              <a href="/" class="active firstCh">
                                <span>Home</span>
                                <i class="icon-home icon-2x"></i>
                              </a>
                            </li>
                             <li>
                              <a href="/check" class="secondCh">
                                <span>Check Your Stock</span>
                                <i class="icon-envelop icon-2x"></i>
                              </a>
                            </li>
                         
                            
                            <li>
                              <a href="/stock">
                                <span>STOCKS</span>
                                <i class="icon-globe  icon-2x"></i>
                              </a>
                            </li>
                            <li>
                              <a href="#about" class="lastCh">
                                <span>WHY US ?</span>
                                <i class="icon-group icon-2x"></i>
                              </a>
                            </li>  
                          </ul>
                          
                    </nav> <!-- end menu -->
                  
                  </div>   <!-- end menu-wrap -->
                  
               </div> <!-- end span12 -->
             
             </div>  <!-- end row -->
             
           </div>  <!-- end container -->
     
       </div>   <!-- end top-nav -->
    
    </div>  
    <!-- END HOME -->
      <!-- START CONTACT -->  
     <div class="page" id="contact">
        
        <!-- START CONTACT WRAP -->  
        <div class="contact-wrap"> 
        
         
         
          <div class="container">
           
             <div class="row">
                 
               <div class="contact-inner-wrap">

                 <div class="span6 clearfix">
  
                   <h5 class="contact-title">Check Stock</h5>
                 
                     <form method="post" class="form-main" action="/graphhome" >
                        
                        <div class="group-form">
                        
                            <label for="contact-name" class="control-label">Stock Name:</label>
                          
                           <div class="group">
                           
                                
                                <input type="text" name="stock" id="contact-name" />
                            
                           </div> <!-- end group -->
                            
                         </div>   <!-- end group-form -->
                      
                          
                       
                          
                          <div class="group-form">
                          
                            <input type="submit" class="btn" value="Check" />
                            
                          </div> <!-- end group-form -->
                   </form>
                 
                 </div> <!-- end span6 --> 
                 
                
              
              </div> <!-- end contact-inner-wrap -->
              
           </div> <!-- end row --> 
           
         </div>  <!-- end container -->            
            
       </div> <!-- end contact-wrap -->    
    
     </div> 
     <!-- END CONTACT -->
   
